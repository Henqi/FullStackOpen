{
  "settings": {
    "react": {
     "version": "detect"
    }
  },
  "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
      }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "semi": 0,
        "prefer-destructuring": 0,
        "react/jsx-filename-extension": 0,
        "max-len": 0,
        "react/prop-types": 0,
        "react/destructuring-assignment": 0,
        "array-callback-return": 0,
        "arrow-body-style": 0,
        "no-shadow": 0
      }
}
